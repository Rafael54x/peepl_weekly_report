<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_peepl_staff" model="res.groups">
        <field name="name">Staff</field>
        <field name="category_id" ref="module_category_peepl_weekly_report"/>
    </record>

    <record id="group_peepl_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_peepl_weekly_report"/>
        <field name="implied_ids" eval="[(4, ref('group_peepl_staff'))]"/>
    </record>

    <record id="group_peepl_bod" model="res.groups">
        <field name="name">BOD</field>
        <field name="category_id" ref="module_category_peepl_weekly_report"/>
        <field name="implied_ids" eval="[(4, ref('group_peepl_manager'))]"/>
    </record>
</odoo>
