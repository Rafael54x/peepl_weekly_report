<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_users_form_weekly_report" model="ir.ui.view">
        <field name="name">res.users.form.weekly.report</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='access_rights']//group[1]" position="after">
                <group string="Weekly Reports">
                    <field name="weekly_report_access"/>
                    </group>
            </xpath>
        </field>
    </record>
</odoo>